   "source": [
    "### Converting True & False to 1 & 0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 208,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Booli(x): # converts special true and false strings to  1 & 0\n",
    "    if ~(type(x) == float and np.isnan(x)):\n",
    "        if x == '\"\\'boolean-false\\'\"':\n",
    "            return 0\n",
    "        elif x == '\"\\'boolean-true\\'\"':\n",
    "            return 1\n",
    "        else:\n",
    "            return x\n",
    "    else:\n",
    "        return x"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 209,
   "metadata": {},
   "outputs": [],
   "source": [
    "def convert_01(var_name): #rewrites values by a Booli function\n",
    "    data[var_name] = data[var_name].apply(Booli)  "
   ]
  },
